<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>show users</title>
</head>
<body>
    <table border="1">
        <tr>
            <th>id</th>
            <th>username</th>
            <th>email</th>
            <th>edit</th>
            <th>delete</th>
        </tr>
        <tr>
            
<% for(let user of result){ %>
    <tr>
        <td><%= user.id %></td>
        <td><%= user.username %></td>
        <td><%= user.email%></td>
        <td><a href="/user/<%=user.id%>/edit">edit</a></td>
        <td><a href="/user/<%=user.id%>/delete">delete</a></td>
    </tr>
    <% } %>
        </tr>
    </table>
</body>
</html>
